<?xml version="1.0" encoding="utf-8" ?>
<UniversalEditor Version="4.0">
	<Associations>
		<Association>
			<Filters>
				<Filter Title="Slightly Mad Studios BFF archive" ContentType="application/x-slightlymadstudios-bff" HintComparison="MagicThenFilter">
					<FileNameFilters>
						<FileNameFilter>*.bff</FileNameFilter>
					</FileNameFilters>
					<MagicByteSequences>
						<MagicByteSequence>
							<MagicByte Type="String"> KAP</MagicByte>
						</MagicByteSequence>
					</MagicByteSequences>
				</Filter>
			</Filters>
			<ObjectModels>
				<ObjectModel TypeName="UniversalEditor.ObjectModels.FileSystem.FileSystemObjectModel" />
			</ObjectModels>
			<DataFormats>
				<DataFormat TypeName="UniversalEditor.DataFormats.FileSystem.SlightlyMadStudios.BFFDataFormat" />
			</DataFormats>
		</Association>
	</Associations>
</UniversalEditor>
