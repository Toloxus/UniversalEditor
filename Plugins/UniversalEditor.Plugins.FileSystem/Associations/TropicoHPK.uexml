<?xml version="1.0" encoding="utf-8" ?>
<UniversalEditor Version="4.0">
	<Associations>
		<Association>
			<Filters>
				<Filter Title="Tropico HPK/BPUL archive">
					<FileNameFilters>
						<FileNameFilter>*.hpk</FileNameFilter>
					</FileNameFilters>
					<MagicByteSequences>
						<MagicByteSequence>
							<MagicByte Type="String">BPUL</MagicByte>
						</MagicByteSequence>
						<MagicByteSequence>
							<MagicByte Type="String">ZLIB</MagicByte>
						</MagicByteSequence>
					</MagicByteSequences>
				</Filter>
			</Filters>
			<ObjectModels>
				<ObjectModel TypeName="UniversalEditor.ObjectModels.FileSystem.FileSystemObjectModel" />
			</ObjectModels>
			<DataFormats>
				<DataFormat TypeName="UniversalEditor.DataFormats.FileSystem.HPK.HPKDataFormat" />
			</DataFormats>
		</Association>
	</Associations>
</UniversalEditor>
