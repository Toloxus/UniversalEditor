<?xml version="1.0" encoding="utf-8" ?>
<UniversalEditor Version="4.0">
	<Associations>
		<Association>
			<Filters>
				<Filter Title="X BitMap (XBM) image" ContentType="image/x-xbitmap" HintComparison="FilterOnly">
					<FileNameFilters>
						<FileNameFilter>*.xbm</FileNameFilter>
					</FileNameFilters>
				</Filter>
				<Filter Title="X PixMap (XPM) image" ContentType="image/x-xpixmap" HintComparison="MagicThenFilter">
					<FileNameFilters>
						<FileNameFilter>*.xpm</FileNameFilter>
					</FileNameFilters>
				</Filter>
			</Filters>
			<ObjectModels>
				<ObjectModel TypeName="UniversalEditor.ObjectModels.Multimedia.Picture.PictureObjectModel" />
			</ObjectModels>
			<DataFormats>
				<DataFormat TypeName="UniversalEditor.DataFormats.Multimedia.Picture.XPM.XPMDataFormat" />
			</DataFormats>
		</Association>
	</Associations>
</UniversalEditor>
